{
  "name": "berry-orm",
  "version": "0.1.0",
  "description": "Object Relational Mapping for front-ends.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:diff": "prettier --list-different \"src/**/*.ts\"",
    "prebuild": "rimraf lib",
    "build": "tsc -b tsconfig.build.json",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint": "eslint --cache \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheNightmareX/berry-orm.git"
  },
  "keywords": [
    "orm",
    "database",
    "typescript",
    "frontend"
  ],
  "author": {
    "name": "Char2s",
    "email": "Char2s@outlook.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/TheNightmareX/berry-orm/issues"
  },
  "homepage": "https://github.com/TheNightmareX/berry-orm#readme",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "vuepress": "^1.8.2",
    "vuepress-plugin-redirect": "^1.2.5"
  },
  "files": [
    "src",
    "lib",
    "!__test__",
    "!tsconfig.*",
    "README*.md"
  ],
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
